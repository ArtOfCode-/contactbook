<!DOCTYPE html>
<html>
<head>
  <title>Contactbook</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if not flash.nil? %>
  <span class="notice-<%= flash[:color] %>">
    <%= flash[:notice] %>
  </span>
<% end %>

<%= yield %>

</body>
</html>
